var mongoose = require( 'mongoose' );

var campuschampSchema = new mongoose.Schema({
  firstname: {
    type: String,
    required: true
  },
  lastname: {
    type: String,
    required: true
  },
  email: {
    type: String,
    required: true
  },
});


mongoose.model('Campuschamp', campuschampSchema);
